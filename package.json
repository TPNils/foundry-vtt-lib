{
  "name": "nils-library",
  "version": "1.1.0",
  "main": "index.js",
  "scripts": {
    "build": "foundry-vtt-build-tools build",
    "package": "foundry-vtt-build-tools buildZip",
    "compileReadme": "foundry-vtt-build-tools compileReadme",
    "dev": "foundry-vtt-build-tools watch",
    "dev11": "foundry-vtt-build-tools watch -fi v11",
    "dev12": "foundry-vtt-build-tools watch -fi v12",
    "patch": "foundry-vtt-build-tools publish -u patch",
    "minor": "foundry-vtt-build-tools publish -u minor",
    "major": "foundry-vtt-build-tools publish -u major",
    "reupload": "foundry-vtt-build-tools reupload",
    "updateZipManifestForGithub": "foundry-vtt-build-tools updateZipManifestForGithub",
    "updateExternalManifestForGithub": "foundry-vtt-build-tools updateExternalManifestForGithub"
  },
  "author": "",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "foundry-vtt-build-tools": "github:TPNils/foundry-vtt-build-tools#v0.1.1"
  }
}
